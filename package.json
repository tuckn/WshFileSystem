{
  "name": "@tuckn/WshFileSystem",
  "description": "Defines Wsh.FileSystem and Wsh.FileSystemExtra objects, and adds functions to handle file and directory. (similar to Node.js-FileSystem).",
  "version": "2.0.0",
  "author": "Tuckn <tuckn333+github@gmail.com>",
  "private": false,
  "license": "MIT",
  "homepage": "https://docs.tuckn.net/WshFileSystem",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuckn/WshFileSystem.git"
  },
  "bugs": {
    "url": "https://github.com/tuckn/WshFileSystem/issues"
  },
  "keywords": [
    "Windows",
    "WSH",
    "WSF",
    "JScript",
    "module",
    "file",
    "directory"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/bundle.js",
  "bin": {},
  "scripts": {
    "install:WshJest": "git submodule add https://github.com/tuckn/WshJest.git ./WshModules/WshJest",
    "install:WshOS": "git submodule add https://github.com/tuckn/WshOS.git ./WshModules/WshOS",
    "installModules": "run-s install:*",
    "updateModules": "git submodule update --remote",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-p fix:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml,yaml}\"",
    "lint:type": "tsc --noEmit",
    "lint": "run-p lint:*",
    "test:name:constants": ".\\scripts\\Test FileSystem -t constants$",
    "test:name:inspectPathWhetherMAX_PATH": ".\\scripts\\Test FileSystem -t inspectPathWhetherMAX_PATH$",
    "test:name:mkdirSync": ".\\scripts\\Test FileSystem -t mkdirSync$",
    "test:name:writeFileSync": ".\\scripts\\Test FileSystem -t ^^writeFileSync",
    "test:name:writeTmpFileSync": ".\\scripts\\Test FileSystem -t ^^writeTmpFileSync",
    "test:name:existsSync": ".\\scripts\\Test FileSystem -t existsSync$",
    "test:name:statSync": ".\\scripts\\Test FileSystem -t statSync$",
    "test:name:readFileSync": ".\\scripts\\Test FileSystem -t ^^readFileSync",
    "test:name:readdirSync": ".\\scripts\\Test FileSystem -t readdirSync$",
    "test:name:copyFileSync": ".\\scripts\\Test FileSystem -t copyFileSync$",
    "test:name:xcopySync": ".\\scripts\\Test FileSystem -t xcopySync$",
    "test:name:linkSync": ".\\scripts\\Test FileSystem -t ^^linkSync",
    "test:name:rmdirSync": ".\\scripts\\Test FileSystem -t rmdirSync$",
    "test:name:unlinkSync": ".\\scripts\\Test FileSystem -t unlinkSync$",
    "test:FileSystem": ".\\scripts\\Test FileSystem",
    "test:name:ensureDirSync": ".\\scripts\\Test FileSystemExtra -t ensureDirSync$",
    "test:name:removeSync": ".\\scripts\\Test FileSystemExtra -t removeSync$",
    "test:name:copySync": ".\\scripts\\Test FileSystemExtra -t copySync$",
    "test:name:unzipOfficeOpenXML": ".\\scripts\\Test FileSystemExtra -t unzipOfficeOpenXML$",
    "test:name:ensureReadingFile": ".\\scripts\\Test FileSystemExtra -t ensureReadingFile$",
    "test:name:readJsonSync": ".\\scripts\\Test FileSystemExtra -t readJsonSync$",
    "test:name:writeJsonSync": ".\\scripts\\Test FileSystemExtra -t writeJsonSync$",
    "test:name:writeCsvSync": ".\\scripts\\Test FileSystemExtra -t writeCsvSync$",
    "test:name:readCsvSync": ".\\scripts\\Test FileSystemExtra -t readCsvSync$",
    "test:name:readCsvFileAsAssocArray": ".\\scripts\\Test FileSystemExtra -t readCsvFileAsAssocArray$",
    "test:name:ensureRemovingFile": ".\\scripts\\Test FileSystemExtra -t ensureRemovingFile$",
    "test:name:copyWshFilesDefinedInWsf": ".\\scripts\\Test FileSystemExtra -t copyWshFilesDefinedInWsf$",
    "test:name:findRequiredFile": ".\\scripts\\Test FileSystemExtra -t findRequiredFile$",
    "test:name:dirTree": ".\\scripts\\Test FileSystemExtra -t dirTree$",
    "test:name:readdirSyncRecursively": ".\\scripts\\Test FileSystemExtra -t readdirSyncRecursively$",
    "test:name:readdirSyncRecursivelyWithDIR": ".\\scripts\\Test FileSystemExtra -t readdirSyncRecursivelyWithDIR$",
    "test:name:getTruePath": ".\\scripts\\Test FileSystemExtra -t getTruePath$",
    "test:name:calcCryptHash": ".\\scripts\\Test FileSystemExtra -t calcCryptHash$",
    "test:name:isTheSameFile": ".\\scripts\\Test FileSystemExtra -t isTheSameFile$",
    "test:name:compareFilesOfModifiedDate": ".\\scripts\\Test FileSystemExtra -t compareFilesOfModifiedDate$",
    "test:FileSystemExtra": ".\\scripts\\Test FileSystemExtra",
    "build": "wsh-packager bundle ./Package.wsf",
    "test:dist:module": ".\\scripts\\Test dist:module",
    "test:dist:bundle": ".\\scripts\\Test dist:bundle",
    "test": "run-s test:dist:*",
    "predocs": "rimraf ./docs",
    "docs:common": "jsdoc --configure ./jsdoc.json",
    "cp-docs-css": "cpy ./assets/styles/jsdoc.css ./docs/styles/",
    "cp-docs-img": "cpy ./assets/img/**/* ./docs/img/",
    "postdocs": "run-p cp-docs-*",
    "docs": "run-s docs:*",
    "up-build-doc": "run-s updateModules build docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo @FIXME \"npx lint-staged\" fixes all .js!"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "*.{json,css,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@tuckn/insert-gtag": "^1.1.0",
    "@tuckn/wsh-packager": "^1.0.0",
    "@types/activex-scripting": "^1.0.7",
    "@types/windows-script-host": "^5.8.3",
    "cpy-cli": "^3.1.1",
    "docdash": "^1.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
}
